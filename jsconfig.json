{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "~styles/*": ["src/styles/*"],
      "~utils/*": ["src/utils/*"],
      "~helpers/*": ["src/helpers/*"],
      "~hooks/*": ["src/hooks/*"],
      "~components/*": ["src/components/*"],
      "~api/*": ["src/api/*"],
      "~contexts/*": ["src/contexts/*"],
      "~constants/*": ["src/constants/*"],
      "~layout/*": ["src/layout/*"],
      "~sections/*": ["src/sections/*"],
      "~routes/*": ["src/routes/*"],
      "~public/*": ["public/*"]
    }
  }
}
